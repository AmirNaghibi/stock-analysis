	"Function (Export)": {
		"prefix": "fe",
		"body": ["export function ${1:Name}(${2:Props}) {", "  ${3:code}", "}"],
		"description": "Function (Export)"
	},
	"Function (Export Default)": {
		"prefix": "fed",
		"body": [
			"export default function ${1:Name}(${2:Props}) {",
			"\treturn (",
			"\t\t${3:code}",
			"\t);",
			"}"
		],
		"description": "Function (Export Default)"
	},
